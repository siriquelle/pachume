.settings_left_column_avatar{background-color:#fff;position:relative;top:-11px;}
.settings_left_column_avatar_developer{background-color:#d9d9d9;padding:0px 5px;}

.settings_left_column_input_checkbox_submit{background-color:#66CC00;border:3px #000 solid;bottom:16px;color:#000;cursor:pointer;display:block;display:inline;font-family:Courier New, Courier, monospace;font-size:20px;font-weight:bold;left:18px;position:relative;text-align:center;top:7px;width:145px;z-index:2;}
.settings_left_column_input_file{filter:alpha(opacity=0);moz-opacity:.0;opacity:.0;position:relative;top:11px;z-index:2;}
.settings_left_column_input_file_button{background-color:#4999FC;border:3px #000 solid;bottom:17px;color:#000;display:block;display:inline;font-family:Courier New, Courier, monospace;font-size:16px;font-weight:bold;height:29px;left:-5px;position:relative;text-align:left;width:75px;}
.settings_left_column_input_file_container{display:inline;left:11px;position:relative;}
.settings_left_column_input_file_submit{background-color:#66CC00;border:3px #000 solid;bottom:16px;color:#000;cursor:pointer;display:block;display:inline;font-family:Courier New, Courier, monospace;font-size:20px;font-weight:bold;left:18px;position:relative;text-align:center;width:145px;z-index:2;}
.settings_left_column_input_file_text{background-color:#FAFFBD;border:3px #000 solid;bottom:16px;font-family:Courier New, Courier, monospace;font-size:18px;font-weight:bold;left:-2px;position:relative;width:148px;}
.settings_left_column_input_label, .settings_left_column_avatar{background-color:#fff;border:3px #000 solid;display:block;float:left;font-family:arial;font-weight:bold;margin:5px;min-width:200px;padding:5px;text-align:center;}
.settings_left_column_input_text{background-color:#FAFFBD;border:3px #000 solid;bottom:16px;font-family:Courier New, Courier, monospace;font-size:18px;font-weight:bold;left:-2px;padding-left:5px;position:relative;text-align:left;width:225px;}
.settings_left_column_input_text_container{display:inline;left:11px;position:relative;top:24px;}
.settings_left_column_input_text_submit{background-color:#66CC00;border:3px #000 solid;bottom:16px;color:#000;cursor:pointer;display:block;display:inline;font-family:Courier New, Courier, monospace;font-size:20px;font-weight:bold;left:15px;position:relative;text-align:center;top:7px;width:145px;z-index:2;}

.settings_left_column_item_container{clear:both;margin-top:5px;}

.settings_left_column_tab_container{background-color:#000;border-bottom:1px #000 solid;font-family:Arial Bold;font-size:14px;font-weight:bold;height:34px;}
.settings_left_column_tab_item{background-color:#fff;border:2px #444 solid;cursor:pointer;float:left;margin-left:15px;margin-top:5px;padding:5px;}
.settings_left_column_top_head_link{background-color:#343434;color:#fff;padding-left:4px;padding-right:4px;text-decoration:none;}
.settings_left_column_top_head_link:hover{background-color:#282828;}

.settings_right_column_middle_body_container{margin-left:auto;margin-right:auto;margin-top:6px;width:206px;}
.settings_right_column_middle_head{background-color:#000;color:#fff;font-family:arial;font-weight:bold;height:24px;margin-top:14px;padding-left:22px;}
.settings_right_column_middle_head_text{position:relative;top:2px;}

.settings_right_column_statistics_number:hover{background-color:#EAEAEA;cursor:pointer;padding:0px 10px;}
.settings_right_column_statistics_statistic{background-color:#fff;border-bottom:3px #000 solid;border-left:3px #000 solid;font-family:Courier New;font-size:48px;font-weight:bold;padding-left:18px;}
.settings_right_column_statistics_title{background-color:#000;color:#fff;font-family:Courier New;font-size:18px;font-weight:bold;padding-left:6px;}

.settings_right_column_top_body_container{margin-left:auto;margin-right:auto;margin-top:6px;width:206px;}