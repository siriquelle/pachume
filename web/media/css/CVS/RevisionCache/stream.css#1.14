.stream_navigation{background-color:#fff;border:3px #000 solid;top:8px;color:#cccccc;font-family:arial;font-size:12px;height:17px;margin-left:auto;padding:10px;position:relative;right:30px;text-align:center;max-width:140px; min-width:70px;}
.stream_navigation a{color:#fff;font-weight:bold;text-decoration:none;}
.stream_navigation a:hover{border-color:#666;}
.stream_navigation_new{background-color:#000;border-color:#000;border-style:solid;border-width:2px 3px 2px 3px;padding:3px;position:relative;top:1px;}
.stream_navigation_new:hover{background-color:#282828;}
.stream_navigation_old{background-color:#000;border-color:#000;border-style:solid;border-width:2px 3px 2px 3px;padding:3px;position:relative;top:1px;}
.stream_navigation_old:hover{background-color:#282828;}

.stream_presence_body_container{background-color:#fff;border:3px #000 solid;margin-left:58px;min-height:50px;position:relative;top:-56px;width:496px;}

.stream_presence_body_meta{color:#999;font-family:arial;font-size:0.6em;height:14px;margin-left:5px;padding-bottom:0.2em;}
.stream_presence_body_meta .delete_pachume, .stream_presence_body_meta .delete_comment{background-image:url(/media/images/delete_pachume.png);background-position:bottom left;background-repeat:no-repeat;padding-left:10px;}
.stream_presence_body_meta a{color:#aabbff;font-family:arial;text-decoration:none;}
.stream_presence_body_meta_comment_count{color:#c00;}

.stream_presence_body_pachume{font-family:'Courier New', Courier, monospace;font-size:20px;font-weight:bold;margin:5px;min-height:24px;overflow:hidden;}
.stream_presence_body_pachume a.inlink{background-position:bottom;background-repeat:repeat-x;color:#000000;font-size:18px;text-decoration:none;}
.stream_presence_body_pachume a.inlink:hover{background-image:url(/media/images/link_bg_hover.png);color:#000000;}
.stream_presence_body_pachume a.inlink:visited{color:#021D3E;}
.stream_presence_body_pachume a.outlink{background-image:url(/media/images/link_bg_default.png);background-position:bottom;background-repeat:repeat-x;color:#85BBFD;font-size:18px;text-decoration:none;}
.stream_presence_body_pachume a.outlink:hover{background-image:url(/media/images/link_bg_hover.png);color:#BBE88D;}
.stream_presence_body_pachume a.outlink:visited{color:#C2DDFE;}

.stream_presence_body_tags_active{color:#fff;font-family:Trebuchet, Helvetica, sans-serif;font-size:10px;margin-left:3px;}
.stream_presence_body_tags_active a{background-color:#444;color:#fff;padding:0px 2px 0px 2px;text-decoration:none;}
.stream_presence_body_tags_active a:hover{background-color:#444;}
.stream_presence_body_tags_default{color:#fff;font-family:Trebuchet, Helvetica, sans-serif;font-size:10px;font-weight:bold;margin-left:3px;}
.stream_presence_body_tags_default a{background-color:#000;color:#fff;padding:0px 2px 0px 2px;text-decoration:none;}
.stream_presence_body_tags_default a:hover{background-color:#444;}

.stream_presence_container_default{margin-left:40px;margin-top:15px; margin-bottom:-60px;min-height:50px;}

.stream_presence_image{background-color:#66CC00;border:5px #4a0 solid;}
.stream_presence_image_container{background-color:#390;border:3px #000 solid;height:50px;width:50px;}